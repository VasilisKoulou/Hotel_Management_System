@model IEnumerable<Hotel.Domain.Entities.ApplicationUser>


@{
    ViewData["Title"] = "Users";
}

<h1>Users</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Email</th>
            <th>UserName</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.FirstName</td>
                <td>@user.Email</td>
                <td>@user.UserName</td>
                <td>
                   
                </td>
            </tr>
        }
    </tbody>
</table>
